    ["gh"]="completion -s zsh"
    ["golangci-lint"]="completion zsh"
    ["ionic"]="completion"
    ["kompose"]="completion zsh"
    ["kubebuilder"]="completion zsh"
    ["kubectl"]="completion zsh"
    ["netlify"]="completion:generate --shell=zsh"
